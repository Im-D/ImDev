{
  "name": "im-d-blog",
  "version": "1.0.1",
  "description": "Im-D Dev Blog",
  "author": "Im-D <im-d-team@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "dev": "npm run clean & gatsby develop",
    "deploy": "gh-pages -d public",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:js && npm run lint:css",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:md": "remark .",
    "lint:mdtest": "node tools/update-post-date.js && remark -o",
    "lint:scss": "stylelint src/**/*.scss --syntax scss",
    "lint:fix": "eslint . --fix",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts"
  },
  "dependencies": {
    "@types/lodash": "^4.14.151",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.8",
    "@types/semantic-ui": "^2.2.7",
    "babel-eslint": "^10.1.0",
    "change-case": "^4.1.1",
    "gatsby": "^2.21.33",
    "gatsby-image": "^2.4.4",
    "gatsby-link": "^2.4.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-glamor": "^2.3.1",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-offline": "^3.2.2",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-autolink-headers": "^2.3.2",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-images": "^3.3.3",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-json": "^2.4.2",
    "gatsby-transformer-remark": "^2.8.8",
    "gatsby-transformer-sharp": "^2.5.2",
    "graphql-code-generator": "^0.18.2",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "redux-devtools-extension": "^2.13.8",
    "semantic-ui-react": "^0.88.2",
    "slash": "^3.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook-readme": "^5.0.4",
    "@types/webpack-env": "^1.15.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "flat": "^5.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.3",
    "mkdirp": "^1.0.4",
    "plop": "^2.6.0",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "remark-cli": "^8.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "stylelint": "^13.4.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.0.0",
    "ts-lint": "^4.5.1"
  },
  "keywords": [],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint . --ext .ts,.tsx --fix",
      "eslint . --ext .js,.jsx --fix",
      "git add"
    ],
    "*.md": [
      "remark . -o",
      "node tools/update-post-date.js",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
