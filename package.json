  "keywords": [],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint . --ext .ts,.tsx --fix",
      "eslint . --ext .js,.jsx --fix",
      "git add"
    ],
    "*.md": [
      "remark . -o",
      "node tools/update-post-date.js",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
